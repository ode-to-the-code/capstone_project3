<div class="container">
  <h3>My Subscriptions (Deeds To Work On)</h3>
  <% @user.subscriptions.each do |subscription| %>
    <div class="well">
      <p> Deed: <%= subscription.deed.name %></p>
      <p> Description: 
        <%= subscription.deed.description %> 
      </p>
    </div>
  <% end %>

  <h3>Deeds I've Posted</h3>
  <% @deeds.each do |deed| %>
    <div class="well">
      <h2> Name: <%= deed.name %></h2>
      <h3> Description: </h3> <br> 
      <%= deed.description %> </p>
      <br> 
      <h3> Comments: </h3>
      <% deed.comments.each do |comment| %>
           <p> <%= comment.body %></p>
           <p> <%= comment.user.name %></p>
      <% end %>
    </div>
  <% end %>
</div>

